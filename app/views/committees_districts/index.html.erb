<p id="notice"><%= notice %></p>

<h1>Listing Committees Districts</h1>

<table>
  <thead>
    <tr>
      <th>Votes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @committees_districts.each do |committees_district| %>
      <tr>
        <td><%= committees_district.votes %></td>
        <td><%= link_to 'Show', committees_district %></td>
        <td><%= link_to 'Edit', edit_committees_district_path(committees_district) %></td>
        <td><%= link_to 'Destroy', committees_district, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Committees district', new_committees_district_path %>
